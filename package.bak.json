{
  "name": "AAchat",
  "version": "1.2.0",
  "author": "AAchat",
  "description": "AAchat",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build:nsis:dev": "node ./nsis-release/release.js",
    "build:nsis:test": "cross-env ENV_CONFIG=test node ./nsis-release/release.js",
    "build:nsis:prod": "cross-env ENV_CONFIG=prod node ./nsis-release/release.js",
    "build:dev": "node ./electron-builder-config/build.js && cross-env ENV_CONFIG=dev node --max_old_space_size=4096 .electron-vue/build.js && electron-builder --config ./electron-builder-config/AAchat.json && npm run build:nsis:dev",
    "build:test": "cross-env ENV_CONFIG=test node ./electron-builder-config/build.js && cross-env ENV_CONFIG=test node --max_old_space_size=4096 .electron-vue/build.js && electron-builder --config ./electron-builder-config/MessChat.json && npm run build:nsis:test",
    "build:prod": "cross-env ENV_CONFIG=prod node ./electron-builder-config/build.js && cross-env ENV_CONFIG=prod node --max_old_space_size=4096  .electron-vue/build.js && electron-builder --config ./electron-builder-config/AAchat.json && npm run build:nsis:prod",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev:AAchat": "node .electron-vue/dev-runner.js",
    "dev:MessChat": "cross-env ENV_CONFIG=test node .electron-vue/dev-runner.js",
    "dev:prod": "cross-env ENV_CONFIG=prod node .electron-vue/dev-runner.js",
    "dev:web": "node .electron-vue/dev-runner-web.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production ENV_CONFIG=prod node --max_old_space_size=1024 node_modules/webpack/bin/webpack.js --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production ENV_CONFIG=prod node --max_old_space_size=1024 node_modules/webpack/bin/webpack.js --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "macOS",
    "Windows",
    "AAchat"
  ],
  "dependencies": {
    "@journeyapps/sqlcipher": "^4.0.0",
    "ffi": "^2.3.0"
  },
  "devDependencies": {
    "sqlite3": "^4.0.9",
    "ajv": "^6.5.0",
    "ant-design-vue": "^1.3.10",
    "asar": "^2.0.1",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^5.0.8",
    "electron-builder": "^20.19.2",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^4.0.0",
    "electron-notarize": "^0.1.1",
    "electron-rebuild": "^1.8.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.4.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lg-autoplay": "^1.0.4",
    "lg-fullscreen": "^1.1.0",
    "lg-hash": "^1.0.4",
    "lg-pager": "^1.0.2",
    "lg-share": "^1.1.0",
    "lg-thumbnail": "^1.1.0",
    "lg-video": "^1.2.2",
    "lg-zoom": "^1.1.0",
    "lightgallery": "^1.6.12",
    "lodash": "^4.17.14",
    "mini-css-extract-plugin": "0.4.0",
    "moment-timezone": "^0.5.26",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-machine-id": "^1.1.12",
    "node-sass": "^4.13.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "sass-loader": "^7.0.3",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue": "^2.5.16",
    "vue-async-computed": "^3.7.0",
    "vue-devtools": "^5.1.3",
    "vue-electron": "^1.0.6",
    "vue-html-loader": "^1.2.4",
    "vue-i18n": "^8.14.0",
    "vue-loader": "^15.2.4",
    "vue-qriously": "^1.1.1",
    "vue-ref": "^1.0.6",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "vuescroll": "^4.14.4",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0",
    "vuex-router-sync": "^5.0.0",
    "wangeditor": "^3.1.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.2.1"
  }
}